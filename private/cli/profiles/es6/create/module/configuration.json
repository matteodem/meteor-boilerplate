{
  "variables" : [
    {
      "name" : "moduleName",
      "description" : "What's the name of your module?"
    }
  ],
  "files" : [
    {
      "template" : "module.html",
      "path" : "client/modules/__moduleName__/__moduleName__.html"
    },
    {
      "template" : "module.next.js",
      "path" : "client/modules/__moduleName__/__moduleName__.next.js"
    },
    {
      "template" : "module.less",
      "path" : "client/modules/__moduleName__/__moduleName__.less"
    }
  ]
}
